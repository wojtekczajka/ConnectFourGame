<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">

<servlet>  
<servlet-name>s1</servlet-name>  
<servlet-class>com.servlets.SessionCreator</servlet-class>  
</servlet>
  
<servlet-mapping>  
<servlet-name>s1</servlet-name>  
<url-pattern>/servlet1</url-pattern>  
</servlet-mapping>  
    
<servlet>  
<servlet-name>s2</servlet-name>  
<servlet-class>com.servlets.SingleGame</servlet-class>  
</servlet>  
    
<servlet-mapping>  
<servlet-name>s2</servlet-name>  
<url-pattern>/single_game</url-pattern>  
</servlet-mapping>  

<servlet>  
<servlet-name>s3</servlet-name>  
<servlet-class>com.servlets.LocalGame</servlet-class>  
</servlet> 

<servlet-mapping>  
<servlet-name>s3</servlet-name>  
<url-pattern>/local_game</url-pattern>  
</servlet-mapping>  

<servlet>  
<servlet-name>s4</servlet-name>  
<servlet-class>com.servlets.OnlineGame</servlet-class>  
</servlet> 

<servlet-mapping>  
<servlet-name>s4</servlet-name>  
<url-pattern>/multi_player</url-pattern>  
</servlet-mapping> 

<servlet>  
<servlet-name>s5</servlet-name>  
<servlet-class>com.servlets.StartConnectFour</servlet-class>  
</servlet>
  
<servlet-mapping>  
<servlet-name>s5</servlet-name>  
<url-pattern>/start</url-pattern>  
</servlet-mapping>  
 
</web-app> 

